{
  "poll_interval": 60,
  "modules": [
    {
      "name": "portscanner",
      "enabled": true,
      "functions": {
        "discover_ips": ["192.168.1"],
        "scan_common_ports": ["192.168.1.59", true],
        "scan_target": {
          "target_ip": "192.168.1.59",
          "ports": [21, 22, 80, 443],
          "range_from": 1,
          "range_to": 1024,
          "threaded": true
        }
      }
    },
    {
      "name": "system_enumeration",
      "enabled": true,
      "functions": {
        "get_active_connections": [],
        "get_system_info": [],
        "list_running_processes": []
      }
    }
  ]
}
